{
    "name": "dartsass-plugin-common",
    "version": "0.12.5",
    "license": "MIT",
    "description": "Common Library code to compile sass files using Dart/JS compiler",
    "main": "dist/main.js",
    "types": "lib/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/codelios/dartsass-plugin-common.git"
    },
    "keywords": [
        "scss compiler plugin",
        "sass compiler plugin",
        "sass watcher",
        "scss watcher",
        "dartsass plugin",
        "sass",
        "scss",
        "css",
        "vsce",
        "vsce plugin",
        "vsce sass plugin",
        "vsce scss plugin"
    ],
    "scripts": {
        "sass": "sass",
        "prepublishOnly": "yarn prod",
        "clean": "rm -fr lib",
        "lint": "eslint src --ext ts && eslint __tests__ --ext ts",
        "fix": "eslint --fix . --ext .ts",
        "build": "webpack --mode development",
        "prod": "rm -fr node_modules/fsevents && yarn build && webpack --mode production",
        "testonly": "mocha --timeout 5000 -r ts-node/register __tests__/**/*.spec.ts",
        "test": "yarn dev && yarn testonly"
    },
    "dependencies": {
        "autoprefixer": "^10.4.4",
        "browserslist": "^4.20.2",
        "chokidar": "3.5.3",
        "clean-css": "5.3.0",
        "fibers": "5.0.1",
        "postcss": "8.4.12",
        "sass": "^1.49.11"
    },
    "devDependencies": {
        "@types/chai": "4.3.0",
        "@types/clean-css": "4.2.5",
        "@types/fibers": "3.1.1",
        "@types/mocha": "9.1.0",
        "@types/node": "17.0.23",
        "@types/sass": "1.43.1",
        "@typescript-eslint/eslint-plugin": "5.17.0",
        "@typescript-eslint/parser": "5.17.0",
        "chai": "4.3.6",
        "eslint": "8.12.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-prettier": "4.0.0",
        "mocha": "9.2.2",
        "node-gyp": "9.0.0",
        "node-loader": "^2.0.0",
        "prettier": "2.6.2",
        "ts-loader": "9.2.8",
        "ts-node": "10.7.0",
        "typescript": "4.6.3",
        "webpack": "5.71.0",
        "webpack-cli": "4.9.2"
    }
}
