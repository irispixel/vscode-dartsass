{
  "name": "dartsass-plugin-common",
  "version": "0.12.6",
  "license": "MIT",
  "description": "Common Library code to compile sass files using Dart/JS compiler",
  "main": "dist/main.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/irispixel/vscode-dartsass.git"
  },
  "keywords": [
    "scss compiler plugin",
    "sass compiler plugin",
    "sass watcher",
    "scss watcher",
    "dartsass plugin",
    "sass",
    "scss",
    "css",
    "vsce",
    "vsce plugin",
    "vsce sass plugin",
    "vsce scss plugin"
  ],
  "scripts": {
    "sass": "sass",
    "prepublishOnly": "yarn prod",
    "clean": "rimraf lib",
    "lint": "prettier --check --plugin-search-dir=. . && eslint src --ext ts && eslint __tests__ --ext ts",
    "format": "prettier --write --plugin-search-dir=. .",
    "fix": "eslint --fix . --ext .ts",
    "build": "webpack --mode development",
    "dev": "yarn dev",
    "prod": "rm -fr node_modules/fsevents && yarn build && webpack --mode production",
    "test": "mocha --timeout 5000 -r ts-node/register __tests__/**/*.spec.ts",
    "build:test": "yarn build && yarn test"
  },
  "dependencies": {
    "autoprefixer": "^10.4.7",
    "browserslist": "^4.21.1",
    "chokidar": "3.5.3",
    "clean-css": "5.3.0",
    "postcss": "8.4.14",
    "sass": "^1.53.0"
  },
  "devDependencies": {
    "@types/chai": "4.3.1",
    "@types/clean-css": "4.2.5",
    "@types/fibers": "3.1.1",
    "@types/mocha": "9.1.1",
    "@types/node": "18.0.1",
    "@types/sass": "1.43.1",
    "@typescript-eslint/eslint-plugin": "5.30.4",
    "@typescript-eslint/parser": "5.30.4",
    "chai": "4.3.6",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "mocha": "10.0.0",
    "node-gyp": "9.0.0",
    "node-loader": "^2.0.0",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "ts-loader": "9.3.1",
    "ts-node": "10.8.2",
    "typescript": "4.7.4",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0"
  }
}
